export const HEBREW_LEVELS = [
  { level: 1, name: '×Ö¸×œÖ¶×£', xp: 0, english: 'Alef' },
  { level: 2, name: '×‘ÖµÖ¼×™×ª', xp: 30, english: 'Bet' },
